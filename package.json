{
    "name": "docker-node",
    "private": true,
    "main": "index.js",
    "scripts": {
        "build": "babel src -d dist --presets latest",
        "start": "node dist/index.js",
        "test": "jest",
        "test-w": "jest --watch",
        "test-c": "jest --coverage"
    },
    "dependencies": {
        "babel-preset-latest": "^6.16.0",
        "body-parser": "^1.15.2",
        "express": "^4.14.x",
        "request": "^2.79.0"
    },
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "eslint": "^3.12.2",
        "eslint-config-airbnb-base": "^11.0.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jest": "^1.0.2",
        "jest": "^18.0.0"
    },
    "eslintConfig": {
        "extends": "airbnb-base",
        "plugins": [
            "jest"
        ],
        "env": {
            "es6": true,
            "node": true,
            "jest": true
        },
        "rules": {
            "indent": ["error", 4, {"SwitchCase": 1}],
             "no-console": 0,
             "import/prefer-default-export": 0
        }
    }
}
